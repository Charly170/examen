import java.util.*;
public class Exam1{
public static void main(String args[]){
Scanner sc = new Scanner(System.in); 
  int num,cubo;       
  System.out.println("Introduzca número: ");      
  num=sc.nextInt();
   while(num>=0){ 
    cubo=num*num*num; 
  System.out.println(num + " es igual a "+ cubo);   
  System.out.println("Introduzca otro número: ");      
      num=sc.nextInt();
        }  
       } 
}
